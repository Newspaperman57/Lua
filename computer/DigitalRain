a = 0
aa = 0
b = 0
bb = 0
c = 0
cc = 0
d = 0
dd = 0
e = 0
ee = 0
f = 0
ff = 0
g = 0
gg = 0
h = 0
hh = 0
i = 0
ii = 0
j = 0
jj = 0
k = 0
kk = 0
l = 0
ll = 0
m = 0
mm = 0
n = 0
nn = 0
o = 0
oo = 0
p = 0
pp = 0
q = 0
qq = 0
r = 0
rr = 0
u = 0
uu = 0
s = 0
ss = 0
t = 0
tt = 0
function thematrix()
	x , y = term.getSize()
	y = y + 11
	for i = 1,10 do
		whereto()
	end
	dot1()
	dot2()
	dot3()
	dot4()
	dot5()
	dot6()
	dot7()
	dot8()
	dot9()
	dot10()
	dot11()
	dot12()
	dot13()
	dot14()
	dot15()
	dot16()
	dot17()
	dot18()
	dot19()
	dot20()
	sleep(0.2)
end

term.redirect(peripheral.wrap("top"))
while 1 do 
	thematrix()
end
