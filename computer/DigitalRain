
function whereto()
	local random = math.random(1,y/3)
	if random == 3 then
		local anumber = math.random(1,x)
		if a == 0 then
			a = a +1
			aa = aa -aa +anumber
			elseif b == 0 then
			b = b +1
			bb = bb -bb +anumber
			elseif c == 0 then
			c = c +1
			cc = cc -cc +anumber
			elseif d == 0 then
			d = d +1
			dd = dd -dd +anumber
			elseif e == 0 then	
			e = e +1
			ee = ee -ee +anumber
			elseif f == 0 then
			f = f +1
			ff = ff -ff +anumber
			elseif g == 0 then
			g = g +1
			gg = gg -gg +anumber
			elseif h == 0 then
			h = h +1
			hh = hh -hh +anumber
			elseif i == 0 then
			i = i +1
			ii = ii -ii +anumber
			elseif j == 0 then
			j = j +1
			jj = jj -jj +anumber
			elseif k == 0 then
			k = k +1
			kk = kk -kk +anumber
			elseif l == 0 then
			l = l +1
			ll = ll -ll +anumber
			elseif m == 0 then
			m = m +1
			mm = mm -mm+anumber
			elseif n == 0 then
			n = n +1
			nn = nn -nn +anumber
			elseif o == 0 then
			o = o +1
			oo = oo -oo +anumber
			elseif p == 0 then
			p = p +1
			pp = pp -pp +anumber
			elseif q == 0 then
			q = q +1
			qq = qq -qq +anumber
			elseif r == 0 then
			r = r +1
			rr = rr -rr +anumber
			elseif rs == 0 then
			s = s +1
			ss = ss -ss +anumber
			elseif t == 0 then
			t = t +1
			tt = tt -tt +anumber
			elseif u == 0 then
			u = u +1
			uu = uu -uu +anumber
		end
	end	
end

function dot1()
	if a > 0 then
		term.setCursorPos(aa,a)
		local nr = math.random(33,126)
	    write(string.char(nr))
		term.setCursorPos(aa,a-y/3)
		write(" ")
		a = a +1
		if a == y+y then
			a = a -a 
		end
	end	
end

function dot2()
	if b > 0 then
		term.setCursorPos(bb,b)
		local nr = math.random(33,126)
	    write(string.char(nr))
		term.setCursorPos(bb,b-y/3)
		write(" ")
		b = b +1
		if b == y+y then
			b = b -b 
		end
	end	
end

